<div class="page-header">
  <h3>Food Order #<%= @food_order.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Food</dt>
      <dd>
        <% if @food_order.food.present? %>
          <a href="/food_items/<%= @food_order.food_id %>">
            <%= @food_order.food.name %>
          </a>
        <% end %>
      </dd>

      <dt>Bar</dt>
      <dd>
        <% if @food_order.bar.present? %>
          <a href="/bars/<%= @food_order.bar_id %>">
            <%= @food_order.bar.name %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/food_orders" class="btn btn-primary">
        Back
      </a>
      <a href="/food_orders/<%= @food_order.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_food_order/<%= @food_order.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
